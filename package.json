{
  "name": "express-boilerplate",
  "version": "1.1.0",
  "description": "Express boilerplate project",
  "main": "server.js",
  "repository": {
    "url": "git@github.com:pro100Koss/express-boilerplate.git"
  },
  "scripts": {
    "watch": "nodemon",
    "debug": "nodemon --inspect src/index.ts",
    "dev": "NODE_ENV=development npm run build && node build/index.js",
    "prod": "NODE_ENV=production npm run build && node build/worker.js",
    "test": "tsc && jest --verbose",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:errors": "eslint \"src/**/*.ts\" --quiet",
    "lint:fix": "eslint  \"src/**/*.ts\" --fix",
    "format:check": "prettier --check \"src/**/*.{ts,js}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,js}\"",
    "validate": "yarn run lint && yarn run build && rm -rf ./build",
    "build": "rimraf ./build && tsc && ef-tspm && cp -r ./src/views ./build/views"
  },
  "author": "Kostya Zamurnyak",
  "license": "ISC",
  "engines": {
    "node": "~14.19.3",
    "yarn": "~1.22.17"
  },
  "dependencies": {
    "async": "^2.6.2",
    "await-to-js": "^2.1.1",
    "aws-sdk": "~2.912.0",
    "axios": "^0.21.1",
    "bcrypt": "~5.0.1",
    "body-parser": "~1.20.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "ejs-locals": "^1.0.2",
    "express": "^4.17.1",
    "express-fileupload": "^0.4.0",
    "express-jwt": "^5.3.3",
    "express-mailer": "^0.3.1",
    "express-session": "~1.17.2",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongodb": "^3.7.3",
    "mongoose": "^5.13.14",
    "nanoid": "^3.3.4",
    "node-cron": "^2.0.3",
    "passport": "~0.6.0",
    "serve-favicon": "^2.5.0",
    "url-parse": "^1.5.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@ef-carbon/tspm": "^2.2.5",
    "@types/bcrypt": "^5.0.0",
    "@types/cloudflare": "^2.7.8",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/express-jwt": "^6.0.2",
    "@types/express-session": "^1.17.5",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.170",
    "@types/mongoose": "^5.10.5",
    "@types/node": "~14.17.0",
    "@types/node-cron": "^3.0.0",
    "@types/passport": "^1.0.7",
    "@types/serve-favicon": "^2.5.3",
    "@types/url-parse": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "~8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "~26.6.3",
    "nodemon": "~2.0.4",
    "prettier": "~2.6.2",
    "rimraf": "~3.0.2",
    "ts-jest": "~26.5.6",
    "ts-node": "~8.10.2",
    "tsconfig-paths": "~3.9.0",
    "typescript": "~3.9.10"
  }
}
